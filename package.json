{
    "name": "jswave",
    "productName": "jswave",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": ".webpack/main",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "test": "mocha --require ts-node/register test/*.ts"
    },
    "keywords": [],
    "author": {
        "name": "Kenneth Fang",
        "email": "kwf37@cornell.edu"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "jswave"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/index.html",
                                    "js": "./src/ui/app.tsx",
                                    "name": "main_window"
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "6.0.0-beta.47",
        "@electron-forge/maker-deb": "6.0.0-beta.47",
        "@electron-forge/maker-rpm": "6.0.0-beta.47",
        "@electron-forge/maker-squirrel": "6.0.0-beta.47",
        "@electron-forge/maker-zip": "6.0.0-beta.47",
        "@electron-forge/plugin-webpack": "6.0.0-beta.47",
        "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
        "chai": "^4.2.0",
        "css-loader": "^3.0.0",
        "electron": "7.1.7",
        "fork-ts-checker-webpack-plugin": "^3.1.1",
        "mocha": "^6.2.2",
        "node-loader": "^0.6.0",
        "style-loader": "^0.23.1",
        "ts-loader": "^6.2.1",
        "tslint": "^5.20.0",
        "typescript": "^3.7.0"
    },
    "dependencies": {
        "@types/chai": "^4.2.7",
        "@types/mocha": "^5.2.7",
        "@types/parsimmon": "^1.10.0",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "electron-squirrel-startup": "^1.0.0",
        "parsimmon": "^1.13.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "ts-node": "^8.5.4"
    }
}
